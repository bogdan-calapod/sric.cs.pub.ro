mixin contact
  section.contact
    h1.right Contact
    .socialElements
      a.element(href='#')
        img(src='img/assets/mail.svg')
        span sric@systems.cs.pub.ro
      a.element(href='#')
        img(src='img/assets/map.svg')
        span Precis 708, Campus UPB, #[br] Iuliu Maniu 6F, Bucure»ôti
      a.element(href='#')
        img(src='img/assets/fb.svg')
        span /systems.cs.pub.ro
    .contact
      h2.error
      iframe(id='nodisplay' name='nodisplay')
      form(method='POST' target='nodisplay' data-url='https://docs.google.com/a/tech-lounge.ro/forms/d/e/1FAIpQLSf68J1SANwaWX2k1Z7uqWoYQsfePfp1CuoZVlNl08dV7V4NTA/formResponse?')
        .row
          .input
            label(for='name') Name
            input(type='text' data-id='entry.2043247778' name='name')

          .input
            label(for='surname') Surname
            input(type='text' data-id='entry.1087798805' name='surname')

          .input
            label(for='email') Email
            input(type='email' data-id='entry.1583697866' name='email')

        .textarea
          label(for='message') Message
          textarea(data-id='entry.2120229607' name='message')
      button.arrow#sendButton Send
    iframe(src='https://www.google.com/maps/embed?pb=!1m17!1m8!1m3!1d1424.451017866651!2d26.0480671!3d44.4351717!3m2!1i1024!2i768!4f13.1!4m6!3e6!4m0!4m3!3m2!1d44.434917!2d26.047874!5e0!3m2!1sen!2sro!4v1469615380952', frameborder='0', style='border:0', allowfullscreen='')
